{
  "expo": {
    "name": "Calorie Cam",
    "slug": "calorie-cam",
    "privacy": "public",
    "ios": {
      "bundleIdentifier": "com.heroku.caloriecam",
      "buildNumber": "1.0.0",
      "supportsTablet": false,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan food items in order to recoginze and retrieve their respective nutritional information."
      }
    },
    "android": {
      "package": "com.joshjones.caloriecam",
      "versionCode": 1,
      "permissions": [
        "CAMERA"
      ]
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/iosIcon.png",
    "android.adaptiveIcon.foregroundImage": "./assets/androidIcon.png",
    "splash": {
      "image": "./assets/customEatingPNG.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffe8d6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "description": "Count you calories by taking a picture of your food.",
    "githubUrl": "https://github.com/JoshuaJoness/calorie-cam"
  }
}
